{
  "name": "psdwizard-bootstrap4-template",
  "version": "1.0.0",
  "description": "A boilerplate for Bootstrap projects using Sass 7-1 architecture and JS revealing module patterns.",
  "main": "index.js",
  "scripts": {
    "preinstall": "node -e 'if (process.env.npm_execpath.indexOf('yarn') === -1)throw new Error(\"PLEASE USE YARN INSTEAD :D https://yarnpkg.com/en/docs/install\")'",
    "dev": "gulp --gulpfile ./tasks/index.js",
    "build": "gulp --gulpfile ./tasks/build.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "author": "PSDWizard",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.24.5",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-inject-string": "^1.1.1",
    "gulp-jsmin": "^0.1.5",
    "gulp-remove-html": "^1.3.0",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.0"
  },
  "dependencies": {}
}
